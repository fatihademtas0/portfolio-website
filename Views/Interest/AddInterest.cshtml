@model MvcCvProject.Models.Entity.TblInterests
@{
    ViewBag.Title = "AddInterests";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}
<br />
<h2>Add a Hobby</h2>
<br />

<form method="post">

    <div class="form-group">
        <label>ID</label>
        @Html.TextBoxFor(x => x.id, new { @class = "form-control", @readonly = "readonly" })
    </div>

    <br />

    <div class="form-group">
        <label>Description 1</label>
        @Html.TextBoxFor(x => x.description1, new { @class = "form-control" })
    </div>


    <br />

    <button class="btn btn-info" style="padding: 8px 16px">Save</button>
</form>

