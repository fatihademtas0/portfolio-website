@using MvcCvProject.Models.Entity
@model List<TblEducation>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<br />
<h2>Educations List</h2>
<br />

<table class="table table-bordered">
    <tr>
        <th>ID</th>
        <th>Title</th>
        <th>Department</th>
        <th>GPA</th>
        <th>Date</th>
        <th>Update</th>
        <th>Delete</th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>@item.id</td>
            <td>@item.title</td>
            <td>@item.subtitle</td>
            <td>@item.gpa</td>
            <td>@item.date</td>
            <td><a href="/Education/UpdateEducation/@item.id" class="btn btn-warning">Update</a></td>
            <td><a href="/Education/DeleteEducation/@item.id" class="btn btn-danger">Delete</a></td>
        </tr>

    }

</table>

<a href="/Education/AddEducation" class="btn btn-primary">Add new education</a>

