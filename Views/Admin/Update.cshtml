@model MvcCvProject.Models.Entity.TblAdmin


@{
    ViewBag.Title = "Update";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<br />
<h2>Update User</h2>
<br />

<form method="post">

    <div class="form-group">
        <label>ID</label>
        @Html.TextBoxFor(x => x.id, new { @class = "form-control", @readonly = "readonly" })
    </div>

    <div class="form-group">
        <label>User Name</label>
        @Html.TextBoxFor(x => x.username, new { @class = "form-control" })
    </div>

    <div class="form-group">
        <label>Password</label>
        @Html.TextBoxFor(x => x.password, new { @class = "form-control" })
    </div>

    <br />

    <button class="btn btn-info" style="padding: 8px 16px">Update</button>
</form>


